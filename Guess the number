#include <stdio.h>
#define n 100
int main()
{
	int number;
	printf("%s","Enter your number:");
	scanf("%d",&number);
	if(number==n){
		printf("You win");
	}
	if(number>n){
		printf("Bigger\n");
	}
	if(number<n){
		printf("Smaller\n");
	}
	while(number!=n){
	printf("%s","Enter your number:");
	scanf("%d",&number);
		if(number==n){
		printf("You win");
	}
	if(number>n){
		printf("Bigger\n");
	}
	if(number<n){
		printf("Smaller\n");
	}
	}
} 
