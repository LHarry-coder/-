#include <stdio.h>
void SQUARE(int side,char fillcharacter);
int main(void)
{
	int x;
	char y;
	printf("%s","Enter your things:\n");
	scanf("%d %c",&x,&y);
	SQUARE(x,y);
}
void SQUARE(int side,char fillcharacter)
{
	for(int i=0;i<side;++i){
		for( int z=0;z<side;++z){
			printf("%c",fillcharacter);
		}
		puts("");
	}
}
